<!DOCTYPE html>
<html>

  <head>
    <meta charset="UTF-8">
    <title>List of the registered users.</title>
  </head>
  <body>
    <ol>
      <% users.forEach((user) => { %>
        <li><%= user.name %></li>
      <% }); %>
    </ol>
    <a href="/addUserForm"><button>Add User</button></a>

    <!-- scripts -->
    <script>
      document.getElementById("addUserButton").formAction = "/addUser";
    </script>
  </body>

</html>
